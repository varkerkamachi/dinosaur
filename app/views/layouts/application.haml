!!! html
%html
  %head
    %meta{ :name => "Charset", :content => "UTF-8" }
    %meta{ :name => "viewport", :content => "width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=1;" }
    - if @pagedescription
      %meta{ :name => "Description", :content => "#{@pagedescription}" }
    %title="#{@pagetitle}"

    =stylesheet_link_tag "application", "home", "flexslider", "http://fonts.googleapis.com/css?family=Marcellus|Istok+Web:400,700|Bad+Script"
    = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js", "application", "modernizr", "flexslider"
    =csrf_meta_tags

  %body
    .wrapper
      .header-wrapper.clearfix
        .header-wrapper-inner
          .logo-wrapper
            =link_to("", root_url, html_options = {:id => "logo" })
            //#logo="Dinos!"
      
       
      #container.clearfix
        = yield
      
        =link_to('click', 'javascript:void(0);', :id => 'linkie' )

        #results
          results: 

   
      :javascript
        jQuery('#linkie').click(function() {
          $.get(
            "http://csscode.org/posts/view/11?callback=handleResponse",
              //beforeSend
            handleResponse = function(responseText) {
              $('#results').html(responseText);
            },
            "html"
            )
      
        });
        function x(n) {
          return function(y) {
            console.log(n + y);
          }
        }
        
        var enn = x(5);
        enn(5);
